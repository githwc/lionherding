<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lh.modules.region.mapper.RegionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lh.modules.region.entity.Region">
        <id column="region_id" property="regionId" />
        <result column="region_code" property="regionCode" />
        <result column="region_pcode" property="regionPcode" />
        <result column="region_name" property="regionName" />
        <result column="region_level" property="regionLevel" />
        <result column="region_initial" property="regionInitial" />
        <result column="city_code" property="cityCode" />
        <result column="remark" property="remark" />
        <result column="seq_num" property="seqNum" />
        <result column="province" property="province" />
        <result column="city" property="city" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        region_id, region_code, region_pcode, region_name, region_level, region_initial, city_code, remark, seq_num, province, city
    </sql>

</mapper>
