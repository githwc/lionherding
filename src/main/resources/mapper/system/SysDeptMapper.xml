<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lh.system.mapper.SysDeptMapper">

    <select id="queryTreeList2" resultType="com.lh.common.tree.TreeNode">
        select
            sys_dept_id as id,
            parent_id as pid,
            depart_name as name,
            sort as order_num
        from sys_dept
        where
            del_flag = 0
        order by sort
    </select>
</mapper>
